<h3>New availability?</h3>
<%= simple_form_for([@user, @availability]) do |f| %>
  <div class="form-inputs">
    <%= f.input :start_date, as: :date, html5: true, class: "form-space"  %>
    <%= f.input :end_date, as: :date, html5: true, class: "form-space"  %>
  </div>
  <br>
  <div class="form-actions">
    <%= f.submit "Add", class: "btn btn-primary" %>
  </div>
<% end %>
